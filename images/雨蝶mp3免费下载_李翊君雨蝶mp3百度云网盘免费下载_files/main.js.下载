var ua = navigator.userAgent;
    if (ua.match(/(iPhone|iPod|ios)/i)) {
    //$("#ddbtn").attr("href","https://apps.apple.com/cn/app/%E9%9D%92%E9%9D%92%E9%9F%B3%E4%B9%90/id1579010233");
    }else if (ua.match(/(Android)/i)) {
   // $("#ddbtn").attr("href","https://s2.9ya.net/m9ku/apkdown/mp3.apk?xzmp3");
    }
    
if($.cookie("show") == "showok") {
    return false;
}else{
    var ua = navigator.userAgent;
    if (ua.match(/(iPhone|iPod|ios)/i)) {
    //downurl = "https://apps.apple.com/cn/app/%E9%9D%92%E9%9D%92%E9%9F%B3%E4%B9%90/id1579010233";
    } else if (ua.match(/(Android)/i)) {
    	//window.setTimeout("window.location='https://s2.9ya.net/m9ku/apkdown/mp3.apk?xzmp3'",1000);
    	$.cookie("show","showok",{path:"/",expires:3})
    }
}
})
